@model HexaControl.Models.CommonQues

@{
    ViewData["Title"] = "Details";
}



<div dir="rtl">

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
           عنوان السؤال 
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Question)
        </dd>
        <dt class="col-sm-2">
            تاريخ النشر
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PubDate)
        </dd>
        <dt class="col-sm-2">
           جواب السؤال
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Answer)
        </dd>
    </dl>


    <div>
        <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model.Id">Edit</a> |
        <a asp-action="Index" class="btn btn-outline-primary">Back to List</a>
    </div>
</div>

